# data
INTEGER
FLOAT
STRING
CHAR
ID

# reserved keyword
FUNCTION        function
RETURN          return
TRUE            true
FALSE           false
IF              if
ELIF            elif
ELSE            else
FI              fi
THEN            then
FOR             for
WHILE           while
DO              do
DONE            done
GLOBAL          global
IMPORT          import
EXPORT          export
MODULE          mod
SIZEOF          sizeof
TYPENAME        typename
REAL            real
VEC             vec
OBJ             obj
BREAK           break
CONTINUE        continue

# structure
COMPOUND

# operator                              priority
NOT             '!'                         1

STAR            '*'                         2
SLASH           '/'                         2
PLUS            '+'                         3
MINUS           '-'                         3
GT              '>'         '-gt'           4
LT              '<'         '-lt'           4
GE              '>='        '-ge'           4
LE              '<='        '-le'           4
EQ              '=='        '-eq'           5
NE              '!='        '-ne'           5
AND             '&&'        '-a'            6
OR              '||'        '-o'            6
EQUAL           '='                         7

# symbol
LPAR            '('
RPAR            ')'
LSQB            '['
RSQB            ']'
LBRACE          '{'
RBRACE          '}'
SEMI            ';'
COMMA           ','
DOLLAR          '$'
COLON           ':'
TO              '->'

# special token for reading program file
END = EOF
NONE = 0
