# data
INTEGER
FLOAT
STRING
CHAR
ID

# reserved keyword
ECHO            
FUNCTION        function
RETURN          return
TRUE            true
FALSE           false
IF              if
ELIF            elif
ELSE            else
FI              fi
THEN            then
FOR             for
WHILE           while
DO              do
DONE            done
GLOBAL          global
IMPORT          import
EXPORT          export
MODULE          mod
SIZEOF          sizeof
TYPENAME        typename
REAL            real
VEC             vec
OBJ             obj

# structure
COMPOUND

# operator
PLUS            '+'
MINUS           '-'
STAR            '*'
SLASH           '/'
EQUAL           '='
NOT             '!'
EQ              '=='        '-eq'
NE              '!='        '-ne'
GT              '>'         '-gt'
LT              '<'         '-lt'
GE              '>='        '-ge'
LE              '<='        '-le'
OR              '||'        '-o'
AND             '&&'        '-a'

# symbol
LPAR            '('
RPAR            ')'
LSQB            '['
RSQB            ']'
LBRACE          '{'
RBRACE          '}'
SEMI            ';'
COMMA           ','
DOLLAR          '$'
COLON           ':'
TO              '->'

# special token for reading program file
END = EOF
NONE = 0
